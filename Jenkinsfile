pipeline {
  agent any
  stages {
    stage('shel') {
      steps {
        sh 'echo \'hey there\''
      }
    }

    stage('NOTIF') {
      steps {
        mail(subject: 'HKJ', body: 'HGffJHGddgtfazdddfzefesjlkj', cc: 'HB@ESI.DZ')
      }
    }

    stage('kjlk') {
      steps {
        sh 'echo \'heyyy\''
      }
    }

  }
}